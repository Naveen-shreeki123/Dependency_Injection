<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <!-- <bean id="emp2" class="co.edureka.Employee" scope="prototype"> -->
    <bean id="adrs" class="DI.Address">
        <property name="city" value="Bangalore"/>
        <property name="state" value="karnataka"/>
        <property name="zipcode" value="560013"/>
    </bean>

    <bean id="c1" class="DI.Customer" init-method="init" destroy-method="destroy">
        <property name="name" value="Seth Rollins"/>
        <property name="address" ref="adrs"/>
<!--        <constructor-arg ref="adrs"/>-->
    </bean>

</beans>